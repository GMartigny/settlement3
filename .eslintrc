{
    "extends": [
        "@gmartigny/eslint-config"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-useless-path-segments": 0,
        "lines-between-class-members": [
            "error",
            "always",
            {
                "exceptAfterSingleLine": true
            }
        ]
    }
}
